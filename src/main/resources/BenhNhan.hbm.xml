        <property name="ngaySinh" type="date">
            <column name="ngay_sinh" />
        </property>
        <property name="gioiTinh" type="string">
            <column name="gioi_tinh" />
        </property>
        
       	<property name="quocTich" type="string">
            <column name="quoc_tich" />
        </property>
        <property name="noiOHienTai" type="string">
            <column name="noi_o_hien_tai" />
        </property>
        <property name="email" type="string">
            <column name="email" />
        </property>
        <property name="soDienThoai" type="string">
            <column name="so_dien_thoai" />
        </property>
        <property name="soCMND" type="string">
            <column name="so_cmnd" />
        </property>
        <property name="soTheBHYT" type="string">
            <column name="so_the_bhyt" />
        </property>
        <property name="queQuan" type="string">
            <column name="que_quan" />
        </property>
       	<property name="ngheNghiep" type="string">
            <column name="nghe_nghiep" />
        </property>

    </class>
</hibernate-mapping>  