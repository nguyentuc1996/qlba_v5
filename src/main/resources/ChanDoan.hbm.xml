<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>
    <class name="com.p3.qlba.entities.ChanDoan" table="chan_doan" schema="dbo" catalog="quan_ly_benh_an" optimistic-lock="version">
        <id name="maChanDoan" type="int">
            <column name="ma_chan_doan" />
            <generator class="identity" />
        </id>

        <property name="maBenhAn" type="int">
            <column name="ma_benh_an" />
        </property>
        <property name="benhChinh" type="string">
            <column name="benh_chinh" />
        </property>
        <property name="benhKemTheo" type="string">
            <column name="benh_kem_theo" />
        </property>
        <property name="ngayChanDoan" type="date">
            <column name="ngay_chan_doan" />
        </property>            
    </class>
</hibernate-mapping>  