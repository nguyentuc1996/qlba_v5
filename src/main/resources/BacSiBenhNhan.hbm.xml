 <hibernate-mapping>
    <class name="com.p3.qlba.entities.BacSiBenhNhan" table="bac_si_benh_nhan" schema="dbo" catalog="quan_ly_benh_an" optimistic-lock="version">
        <id name="maBanGhi" type="int">
            <column name="ma_ban_ghi" />
